{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "rewrites": [
    {
      "source": "/api/uniswap-router",
      "destination": "https://uniswap-quote-backend.riftnodes.com/"
    },
    {
      "source": "/api/uniswap-router/:path*",
      "destination": "https://uniswap-quote-backend.riftnodes.com/:path*"
    }
  ],
  "headers": [
    {
      "source": "/api/uniswap-router(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=0, must-revalidate"
        }
      ]
    }
  ]
}
